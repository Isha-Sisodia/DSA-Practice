# q2
#https://www.geeksforgeeks.org/problems/palindrome-string0817/1
class Solution:
    def isPalindrome(self, s):
        left = 0
        right = len(s)-1
        while left < right:
            if s[left] != s[right]:
                return False
            left += 1
            right -= 1
        return True
# Method - 1
def isPalindrome(s):
    left, right = 0, len(s) - 1
    while left < right:
        if s[left] != s[right]:
            return False
        left += 1
        right -= 1
    return True
s = "abba"
print(isPalindrome(s))

